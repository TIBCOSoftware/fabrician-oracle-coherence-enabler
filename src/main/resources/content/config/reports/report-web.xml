
<report-config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
               xmlns="http://xmlns.oracle.com/coherence/coherence-report-config"
               xsi:schemaLocation="http://xmlns.oracle.com/coherence/coherence-report-config coherence-report-config.xsd">
    <report>
      <file-name>{date}-web.txt</file-name>
      <delim>{tab}</delim>

      <query>
        <pattern>Coherence:type=HttpSessionManager,*</pattern>
      </query>

      <row>
        <column id="BatchCounter">
          <type>global</type>
          <name>{batch-counter}</name>
          <header>Batch Counter</header>
        </column>

        <column id ="ReportTime">
          <type>global</type>
          <name>{report-time}</name>
          <header>Report Time</header>
        </column>

        <column id ="AppId">
          <type>key</type>
          <name>appId</name>
          <header>Application</header>
        </column>

        <column id ="NodeId">
          <type>key</type>
          <name>nodeId</name>
          <header>Node Id</header>
        </column>

        <column id ="NodeTS">
          <name>Timestamp</name>
          <header>Timestamp</header>
          <query>
            <pattern>Coherence:type=Node,nodeId={NodeId}</pattern>
          </query>
          <hidden>true</hidden>
        </column>

        <column id="LocalAttribCnt">
          <name>LocalAttrtibuteCount</name>
          <hidden>false</hidden>
        </column>

        <column id="LocalSessionCnt">
          <name>LocalSessionCount</name>
          <hidden>false</hidden>
        </column>

        <column id="OverflowAvgSize">
          <name>OverflowAverageSize</name>
          <hidden>false</hidden>
        </column>

        <column id="OverflowMaxSize">
          <name>OverflowMaxSize</name>
          <hidden>false</hidden>
        </column>

        <column id="OverflowUpdates">
          <name>OverflowUpdates</name>
          <hidden>false</hidden>
        </column>

        <column id="CurrentOverflowUpdates">
          <type>function</type>
          <name>deltaOverflowUpdates</name>
          <header>Current Overflow Updates</header>
          <column-ref>OverflowUpdates</column-ref>
          <function-name>delta</function-name>
          <params>
            <column-ref>AppId</column-ref>
            <column-ref>NodeId</column-ref>
            <column-ref>NodeTS</column-ref>
          </params>
        </column>

        <column id="SessionAvgLifetime">
          <name>SessionAverageLifetime</name>
          <hidden>false</hidden>
        </column>

        <column id="SessionAvgSize">
          <name>SessionAverageSize</name>
          <hidden>false</hidden>
        </column>

        <column id="OverflowMaxSize">
          <name>OverflowMaxSize</name>
          <hidden>false</hidden>
        </column>

        <column id="SessionMaxSize">
          <name>SessionMaxSize</name>
          <hidden>false</hidden>
        </column>

        <column id="SessionMinSize">
          <name>SessionMinSize</name>
          <hidden>false</hidden>
        </column>

        <column id="SessionStickyCount">
          <name>SessionStickyCount</name>
          <hidden>false</hidden>
        </column>

        <column id="SessionUpdates">
          <name>SessionUpdates</name>
          <hidden>false</hidden>
        </column>

        <column id="CurrentSessionUpdates">
          <type>function</type>
          <name>deltaSessionUpdates</name>
          <header>Current Session Updates</header>
          <column-ref>SessionUpdates</column-ref>
          <function-name>delta</function-name>
          <params>
            <column-ref>AppId</column-ref>
            <column-ref>NodeId</column-ref>
            <column-ref>NodeTS</column-ref>
          </params>
        </column>

      </row>
    </report>
</report-config>